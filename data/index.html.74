            <script>
                jQuery(document).ready(function($){

                    // this grabs the string before the first period (the subdomain), and
                    // splits everything into an array for manipulation
                    var full = window.location.host;
                    var parts = full.split('.');
                    var sub = parts[0];

                    // If the subdomain matches a value in the language drop down, select it. If it doesn't,
                    // it will just select english by default (which is what we want)
                    jQuery('#mySelect').val(sub);

                    jQuery("#mySelect").change(function(){

                        // This grabs the value that was selected
                        var language = $('#mySelect option:selected').val();
                        if (language == 'en'){
                            language = '';
                        } else {
                            language += '.';
                        }

                        var currentUrl = window.location.href;
                        var currentJoin;

                        // This grabs all of the values in the options inside of the select
                        var opts = document.getElementById('mySelect').options;
                        var vals = [];
                        for(var i = 0, j = opts.length; i < j; i++) {
                           vals.push(opts[i].value);
                        }

                        // Loop through the values stashed from all of the options, and check
                        // them against the subdomain. If there is a match, then the page is
                        // translated. If not, the page is not translated

                        var check = function(){

                            if (sub == 'www' || '0' == '2'){
                                // If the url begins with www, strip it
                                var currentUrlParts = currentUrl.split('.');
                                currentUrlParts.splice(0,1);
                                currentJoin = currentUrlParts.join(".");
                                return currentJoin;
                            } else {

                                for (i=0;i<vals.length;i++){
                                     if (sub == vals[i]){
                                     // if the url begins with any known translated page, strip it
                                          var currentUrlParts = currentUrl.split('.');
                                          currentUrlParts.splice(0,1);
                                          currentJoin = currentUrlParts.join(".");
                                          return currentJoin;
                                     }

                                 }

                                // if the url does not begin with www or a translated page, assume
                                // it's a city or an affiliate, and just strip http://
                                currentJoin = currentUrl.replace(/.*?:\/\//g, "");
                                return currentJoin;

                            }

                        };

                        window.location = "http://" + language + check();

                    })

                });
            </script>


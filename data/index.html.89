  <script>
	$(document).ready(function(){
  	$('#gplusDisconnect').click(disconnectUser);
	});
	function disconnectUser()
	{
		var access_token=document.getElementById('accessToken').value;
		var revokeUrl = 'https://accounts.google.com/o/oauth2/revoke?token='+access_token;
		
		// Perform an asynchronous GET request.
  		$.ajax({
    	type: 'GET',
    	url: revokeUrl,
    	async: false,
    	contentType: "application/json",
    	dataType: 'jsonp',
    	success: function(nullResponse) {
		if(document.getElementById('gplus_connect') != null && document.getElementById('customBtn') != 'undefined')
		{
		document.getElementById('gplus_connect').setAttribute('style', 'display: block');
		}
		if(document.getElementById('customBtn') != null && document.getElementById('customBtn') != 'undefined')
		{
	    document.getElementById('customBtn').setAttribute('style', 'display: block');
		}
	   
      	// Do something now that user is disconnected
      	// The response is always undefined.
      		var host = "https://"+document.location.hostname;
      		var url = host+"/GooglePlusAction.do?gpDisconnect=true";
			window.parent.location = url;
      	},
    	error: function(e) {
      	// Handle the error
      	// console.log(e);
      	// You could point users to manually disconnect if unsuccessful
      	// https://plus.google.com/apps
    	}
  		});
	}
	</script>

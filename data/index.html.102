<script id="wisfb_mlbScoreTemplate" type="text/x-handlebars-template">
	<div class="wisfb_game {{GameClass}} {{#if IsSpringGameInRegSeason}}wisfb_springTraining{{/if}}">
	<div class="wisfb_gameContent">
		<header>
			<div class="wisfb_gameTime">{{#if IsMoreThan7DaysAway}}{{DateDayOfWeek}} {{DateSlash}}{{else}}{{TimeText}}{{/if}}<span class="wisfb_tvNetwork">{{#if TvStation}}({{TvStation}}){{/if}}</span></div>
			<div class="wisfb_gameStatus{{#if IsGameStatusSpecial}} wisfb_special{{/if}}"><span>{{StatusText}}</span></div>
			<div class="wisfb_gameNotes">{{GameNotes}}</div>
			<div class="wisfb_liveSeriesText">{{LiveSeriesText}}</div>
			<div class="wisfb_inning {{InningClass}}">
				<img class="wisfb_battingMarker" src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
				<img class="wisfb_inningMarker"  src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
				{{Inning}}
			</div>
			<div class="wisfb_outs {{OutsClass}}">
				<img class="wisfb_out1" src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
				<img class="wisfb_out2" src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
				<img class="wisfb_out3" src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
			</div>
			<div class="wisfb_field {{FieldClass}}">
				<img class="wisfb_runner1" src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
				<img class="wisfb_runner2" src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
				<img class="wisfb_runner3" src="{{wisfb_MLBStyle path='/MLB/Images/sprite.png'}}" />
			</div>
			<div class="wisfb_pitcher">{{Pitching}}</div>
			<div class="wisfb_batter">{{AtBat}}</div>
		</header>
		
		<table class="wisfb_standard wisfb_mlbScoreChip">
			<thead>
				<tr>
					{{#if WinningLogo}}<th class="wisfb_winningLogoHdr"></th>{{/if}}
					<th class="wisfb_teamHdr"></th>
					<th class="wisfb_runsHdr">R</th>
					<th class="wisfb_hitsHdr">H</th>
					<th class="wisfb_errorsHdr">E</th>
					<th class="wisfb_recordHdr"></th>
				</tr>
			</thead>
			<tbody>
				{{#each Teams}}
				<tr {{#if IsLoser}}class="wisfb_losingTeam"{{/if}}>
					{{#if WinningLogo}}{{#if IsAway}}<td class="wisfb_logo" rowspan="2"><img class="wisfb_teamLogoLarge" src="{{WinningLogo}}" alt="Winner" onerror="this.style.display='none'" /></td>{{/if}}{{/if}}
					<td class="wisfb_team">
						<div class="wisfb_fullTeamStacked wisfb_noHover{{#if SpecialCSS}} {{SpecialCSS}}{{/if}}">
							{{#unless WinningLogo}}<img src="{{Logo}}" alt="{{Name}}" onerror="this.style.display='none'"/>{{/unless}}
							<span>{{City}}</span>
							<span>{{Name}}</span>
						</div>
					</td>

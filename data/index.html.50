			function validateEmail(){
			var email = $('.sign-in-input').children('#signinemailid').val();
			var password = $('.sign-in-input').children('#signinpassword').val();
			if(email != null && email != "" && password != ""){
			if(isValidEmailId(email)){
			$('#loginForm').submit();
		        }else{
			$('.sign-in-input').children('.error').show().html("Either the e-mail address or password you entered were incorrect. Please try again.");
			}
			}else if(email != null && email != ""){
			 $('.sign-in-input').children('.error').show().html("Please enter your Password.");
			}else if(password != ""){
			$('.sign-in-input').children('.error').show().html("Please enter your Email Address.");
                        }else{
			$('.sign-in-input').children('.error').show().html("Please enter your Email Address and Password to continue.");
			}
			}

			function isValidEmailId(emailId){
			var regExp = new RegExp(/^[\w_\.\-]+[a-zA-Z0-9]+@[\w.\-]+\.[a-zA-Z]{2,4}$/);
			return regExp.test(emailId);
			}


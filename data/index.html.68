                    (function($){
                        $('.carousel-home').carousel({ transitionOptions: { duration: "normal", easing: "easeOutQuart"} });
                        $('.ct-previous-home').each(function () {
                            var bt = $(this);
                            var c = bt.metadata().carousel;
                            if (c) {
                                bt.click(function (event) {
                                    $(c).carousel('previous'); event.preventDefault();
                                    carouselHomePaginate( $(c).data().carousel.oldIndex, $(c).data().carousel.index);
                                });
                                bt.mouseenter(function() {
                                    if (anim != null) {clearInterval(anim);}
                                });
                            }
                        });

                        $('.ct-next-home').each(function () {
                            var bt = $(this);
                            var c = bt.metadata().carousel;
                            if (c) {
                                bt.click(function (event) {
                                    $(c).carousel('next'); event.preventDefault();
                                    carouselHomePaginate( $(c).data().carousel.oldIndex, $(c).data().carousel.index);
                                });
                                bt.mouseenter(function() {
                                    if (anim != null) {clearInterval(anim);}
                                });
                            }
                        });
	
                        $('.ct-to-home').each(function () {
                            var bt = $(this);
                            var c = bt.metadata().carousel;
                            if (c) {
                                bt.find('li').each(function(i) {
                                    $(this).click(function (event) {
                                        if (i != $(c).data().carousel.index) {	
                                            $(c).carousel('to',{index:i}); event.preventDefault();
                                            carouselHomePaginate( $(c).data().carousel.oldIndex, $(c).data().carousel.index);
                                        }
                                    });
                                    $(this).mouseenter(function() {
                                        if (anim != null) {clearInterval(anim);}
                                    });
                                });
                            }
                        });
	
                        $('.carousel-home').find(".info:first").bind("grow.afterupdate", function (event) {
                            $(this).closest('.carousel-home').hide().show();
                        })
                        .grow({ others: '.info:not(:first)', inClosest: '.carousel-home' });
	
	
                        var h = parseInt($('.carousel-home').find(".info:first").css('height'))-18+'px';
                        $('.carousel-home').find(".info").each(function(){
                            $(this).css('height',h);
                        });
	
 	
                        h = $('.carousel-home').find("li:first").height() + 'px';
                        $('.carousel-home').css('height',h);
                        $('#carousel-pages').find('li:first').css('background-color','#637EBE');
	
                        $('#carousel-pages').find('li').each(function() {
                            $(this).height('6px');
                        });
	
	
                        var anim = setInterval(function() {
                            $('.ct-next-home').click();
                        },5500);
                        $('.carousel-pages').mouseenter(function() {
                            if (anim != null) {clearInterval(anim);}
                        });
                        $('.carousel-home').mouseenter(function() {
                            if (anim != null) {clearInterval(anim);}
                        });


                    function carouselHomePaginate(oldIdx,idx) {
                        if (oldIdx == idx) {
                            return;
                        }
                        $('#carousel-pages').find('li').each(function(i) {
                            if (i == oldIdx) {
                                $(this).css('background-color', '#E9EFEF');
                            } else if (i == idx) {
                                $(this).css('background-color', '#637EBE');
                            }
                        });
                    }
                    })(jQuery);
                </script>

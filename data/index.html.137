<script type="text/javascript">
    function showAccountInfo() {
        FB.api(
            {
                method: 'fql.query',
                query: 'SELECT name, pic FROM user WHERE uid=' + FB.getUserID()
            },
            function(response) {
                $('#account-info').html('<img src="' + response[0].pic + '" style="height: 30px;">');
            }
        );
    }


    function fbLoginStatus(response) {
        CW.log('fbLoginStatus()' , response);
        if (response.status == "connected") {
            showAccountInfo();
        } else {
            //user is not logged in, display guest div

            document.cookie = 'fbsr_132443425037 = ; expires=Thu, 01-Jan-70 00:00:01 GMT; path=/; domain=.indiewire.com';
            refreshNav();
        }
    }
    
    function refreshNav(cb) {
        $('#login').load('/ajax/loginSmall.jsp?_=' + (new Date()).getTime(),
            $.proxy(function() {
                FB.XFBML.parse($('#login').get()[0], function(){
                	var iframe = $('iframe','#login').get()[0];
                	if(iframe){
                		iframe.src = iframe.src;
                	}
                	setTimeout(function(){
                		$('#login').show();
                	}, 500);
                });
                
            }, cb)
        );
    }
    
    function iContactSignup() {
        if(typeof(CW) == 'object') {
            CW.log('iContactSignup()');
        }
        $.ajax({
            type: 'get',
            url: '/ajax/emailsignup.jsp',
            success: function(data) {
                if(typeof(CW) == 'object') {
                    CW.log('emailsignup.jsp: ', data);
                }
            }
        });
    }
    
    var onLikeClicked = null;

    function dropDown() {
        $('.navBar .nav > li, .profile-dropdown').hover(
            function() {
                var dd = $(this).find('div.dropdown');
                // checking dim the lights is necessary for IE
                if (dd.length > 0 && ($('#snag-dimmerBG').filter(":visible").length < 1) ) {
                    $(this).addClass('open');
                    dd.show();
                }
            },
            function() {
                $(this).removeClass('open');
                $(this).find('div.dropdown').hide();
            }
        );
        $('.dropdown .queue-populated li a').click(function(event){
            Snag.Tracking.GA.trackClick(event, "registration and profiles", "watch-q");
        });
    }

    function logout() {
        FB.logout(function(response) {
            document.cookie = 'fbsr_132443425037 = ; expires=Thu, 01-Jan-70 00:00:01 GMT; path=/; domain=.indiewire.com';
            setTimeout(function() {
            	$('#login').hide();
                $('#login').load('/ajax/loginSmall.jsp?_=' + (new Date()).getTime(), 
                		function() {
			                	FB.XFBML.parse($('#login').get()[0], function(){
			                    	setTimeout(function(){
			                    		$('#login').show();
			                    	}, 100);
			                    });
                			});
                if(typeof(CW) == 'object') {
                    CW.user.loggedIn = false;
                    CW.user.critic = false;
                    CW.films.grading.init();
                }
            }, 1000);
        });
    }
    if(IW.FB){
    	IW.FB.callbacks.getLoginStatus.push(fbLoginStatus);
    }
</script>


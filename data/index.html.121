  stories = stories.sort(function() {return 0.5 - Math.random()});
  $(stories).each(function(){
//commas
    var comma1 = '';
    var comma2 = '';
    if (this[2]){
      comma1 = ', '
    };
    if (this[3]){
      comma2 = ', '
    };
//fields
    if (this[0] == null){
     zeroItem = '';
    } else {
      zeroItem = this[0];
    };

    if (this[1] == null){
      firstItem = '';
    } else {
      firstItem = this[1];
    };

    if (this[2] == null){
      secondItem = '';
    } else {
      secondItem = this[2];
    };

    if (this[3] == null){
      thirdItem = '';
    } else {
      thirdItem = this[3];
    };
    inner = '<div class="story story'+ i +'"><p class="quote">' + zeroItem + '</p><div class="attribution">' + firstItem + '</div><div class="source">' + secondItem + '</div><div class="extra">' + thirdItem + '</div></div>';
    $('#story-wrapper').append(inner);
    i = i+1;
  });
  $('#story-wrapper').cycle({
    fx: 'fade',
    timeout: 5000
  });
});

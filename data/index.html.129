});

function callTweetPull() {
	$.ajax({
		url: 'http://abcnews.go.com/xmldata/newsbulletin?id=19432727&lpos=widget[A_4_twitter_4380645_homepage]',
		dataType:"jsonp",
		jsonpCallback:"newsBulletin",

			tweetErrorCount = 0;
			var source   = $("#content-twitter-template").html();
			var template = Handlebars.compile(source);
			var html    = template(data);
			$("#tweet-wrapper").html(html);
		},
		error: function(data) {
			tweetErrorCount++;

			if (parseInt(tweetErrorCount) >= 3) {
				$(".tweet-main-container").remove();
			}
			else {
				setTimeout(function() {
					callTweetPull();
				}, 2000);
			}
		}
	});
}

$(document).ready(function() {

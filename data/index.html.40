                } else {
                    pX = cX;
                    pY = cY;
                    timeout = setTimeout(handler, cfg.interval);
                }
            }

            var timeout = setTimeout(handler, cfg.interval);
            target.mousemove(track).mouseout(clear);
            return true;
        }
    };
